{
  "name": "@kwangmins-blog/util",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./hooks": {
      "import": "./src/hooks/index.ts",
      "require": "./src/hooks/index.ts",
      "types": "./src/hooks/index.ts"
    },
    "./functions": {
      "import": "./src/functions/index.ts",
      "require": "./src/functions/index.ts",
      "types": "./src/functions/index.ts"
    },
    "./types": {
      "import": "./src/types/index.ts",
      "require": "./src/types/index.ts",
      "types": "./src/types/index.ts"
    },
    "./constants": {
      "import": "./src/constants/index.ts",
      "require": "./src/constants/index.ts",
      "types": "./src/constants/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
